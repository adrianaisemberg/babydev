{
  "extends": "tslint-react",
  "rules": {
    "jsx-no-lambda": false,
    "interface-name": false,
    "member-access": [
      true,
      "no-public"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "singleline": "never"
      }
    ]
  }
}